package classwork.evaluatorExample.evaluators;

import classwork.evaluatorExample.Evaluator;

public final class AbsoluteFibunacciEvaluator extends Evaluator{

	@Override
	public Double evaluate() {
		AbsoluteSumEvaluator evaluator = new AbsoluteSumEvaluator();
		double res = evaluator.evaluate(evaluated);
		int prev1 = 0, prev2 = 1;
        for(int i=0; i<n; i++) {
            int savePrev1 = prev1;
            prev1 = prev2;
            prev2 = savePrev1 + prev2;
        }
	}
	
	private int fib(int n) {
		int prev1=0, prev2=1;
        for(int i=0; i<n; i++) {
            int savePrev1 = prev1;
            prev1 = prev2;
            prev2 = savePrev1 + prev2;
        }
        return prev1;
	}
}
