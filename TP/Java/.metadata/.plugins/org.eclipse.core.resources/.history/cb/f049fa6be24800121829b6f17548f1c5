package classwork.interclass;


public class SumEvaluator extends Evaluator {

	@Override
	public void add(double d) {
		eval.add(d);
	}

	@Override
	public Double evaluate() {
		double res = 0;
		if(eval.size() > 0) {
			for(Double d : eval) {
				res +=d;
			}
			eval.removeAll(eval);
			eval.add(res);
			return res;
		}
		return 0.0;
	}

}
