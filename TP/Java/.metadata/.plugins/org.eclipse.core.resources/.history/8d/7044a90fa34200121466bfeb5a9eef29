package org.elsys.testExam;

public class Car {

	private Coordinate position;

	private int regPlate;
	
	public Coordinate getPosition() {
		return position;
	}

	public void setPosition(Coordinate position) {
		this.position = position;
	}
	
	
	public void print() {
		System.out.print("CAR: " + regPlate + " ");
		position.dump();
	}

	public int getRegPlate() {
		return regPlate;
	}

	public void setRegPlate(int regPlate) {
		this.regPlate = regPlate;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + regPlate;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Car other = (Car) obj;
		if (regPlate != other.regPlate)
			return false;
		return true;
	}
	
	
}
