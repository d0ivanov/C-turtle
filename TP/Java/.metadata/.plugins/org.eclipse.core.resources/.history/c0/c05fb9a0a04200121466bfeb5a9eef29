package org.elsys.testExam;

public class Car {

	private Coordinate position;
	private int regPlate;
	
	
	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result
				+ ((position == null) ? 0 : position.hashCode());
		result = prime * result + regPlate;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Car other = (Car) obj;
		if (position == null) {
			if (other.position != null)
				return false;
		} else if (!position.equals(other.position))
			return false;
		if (regPlate != other.regPlate)
			return false;
		return true;
	}



	public void print() {
		System.out.print("CAR: " + regPlate + " ");
		position.dump();
	}
}
